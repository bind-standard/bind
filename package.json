{
  "name": "bind-standard",
  "version": "0.1.0",
  "description": "BIND (Business Insurance Normalized Data) â€” An open data interoperability standard for insurance.",
  "main": "dist/types/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "gen:schema": "ts-node scripts/generate-schemas.ts",
    "gen:docs": "ts-node scripts/generate-docs.ts",
    "gen": "pnpm run gen:schema && pnpm run gen:docs",
    "docs:dev": "pnpm run gen && vitepress dev site",
    "docs:build": "pnpm run gen && vitepress build site",
    "docs:preview": "vitepress preview site",
    "clean": "rm -rf dist schemas/resources schemas/supporting schemas/index.json site/resources site/data-types site/public/schema site/.vitepress/dist site/.vitepress/cache",
    "wrangler:config": "ts-node scripts/generate-wrangler-config.ts",
    "deploy": "pnpm run wrangler:config && pnpm exec wrangler deploy",
    "deploy:delete": "pnpm exec wrangler delete --force",
    "lint": "biome lint .",
    "lint:fix": "biome lint --fix .",
    "format": "biome format --fix .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --fix .",
    "typecheck": "tsc --noEmit",
    "check:docs": "pnpm run gen:schema && ts-node scripts/check-schema-docs.ts"
  },
  "keywords": [
    "bind",
    "insurance",
    "insurance",
    "interoperability",
    "schema",
    "standard"
  ],
  "author": "",
  "license": "CC0-1.0",
  "type": "commonjs",
  "packageManager": "pnpm@10.24.0",
  "devDependencies": {
    "@biomejs/biome": "^2.4.0",
    "@types/d3-drag": "^3.0.7",
    "@types/d3-force": "^3.0.10",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-zoom": "^3.0.8",
    "@types/node": "^25.2.3",
    "d3-drag": "^3.0.0",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "mermaid": "^11.12.2",
    "ts-json-schema-generator": "^2.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.17",
    "wrangler": "^4.14.0"
  }
}
